<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:app="http://schemas.android.com/apk/res-auto"
                xmlns:tools="http://schemas.android.com/tools"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                tools:context=".Ddaibanpaotui.fragment_dabanpaotui.DaiBanPaoTuiFragment">

    <LinearLayout
        android:id="@+id/ll_title_bar"
        android:layout_width="match_parent"
        android:layout_height="@dimen/title_bar_height"
        android:background="@color/title_bar"
        >

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/iv_qicheguanjia_back"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:padding="12dp"
                android:src="@mipmap/ic_header_back"/>

            <TextView
                style="@style/title_bar_text_style"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:layout_gravity="center"
                android:text="汽车管家"/>
        </RelativeLayout>


    </LinearLayout>

    <RelativeLayout
        android:id="@+id/rela_viewpager"
        android:layout_width="match_parent"
        android:layout_height="160dp"
        android:layout_below="@+id/ll_title_bar">

        <android.support.v4.view.ViewPager
            android:id="@+id/vp_headerview_pager"
            android:layout_width="match_parent"
            android:layout_height="160dp">
        </android.support.v4.view.ViewPager>

        <com.aiton.bamin.changtukepiao.ZcustomView.ViewPagerIndicator
            android:id="@+id/ViewPagerIndicator"
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginBottom="10dp"
            app:indicatorCount="3"
            app:layout_position="middle"
            app:radiusSize="5dp"/>
    </RelativeLayout>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/rela_viewpager">

        <RelativeLayout
            android:background="@drawable/layout_selector_white_system_gray"
            android:layout_weight="2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="center_horizontal">

            <ImageView
                android:src="@mipmap/dache"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/imageView9"
                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true"/>
        </RelativeLayout>

        <LinearLayout
            android:layout_marginTop="10dp"
            android:layout_weight="1"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:layout_weight="1.1"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <RelativeLayout
                    android:background="@drawable/layout_selector_white_system_gray"
                    android:id="@+id/rela_zijiazuche"
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_centerHorizontal="true">

                        <ImageView
                            android:src="@mipmap/zijia2x"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/imageView15"
                            android:layout_alignParentTop="true"
                            android:layout_centerHorizontal="true"/>

                        <TextView
                            android:textColor="@color/black"
                            android:textSize="20dp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="自驾租车"
                            android:id="@+id/textView72"
                            android:layout_below="@+id/imageView15"
                            android:layout_centerHorizontal="true"/>
                    </RelativeLayout>
                </RelativeLayout>

                <TextView
                    android:background="@color/line_background"
                    android:layout_width="@dimen/line_width"
                    android:layout_height="match_parent"
                    android:id="@+id/textView162"/>

                <RelativeLayout
                    android:background="@drawable/layout_selector_white_system_gray"
                    android:id="@+id/rela_jigouyongche"
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_centerHorizontal="true">

                        <ImageView
                            android:src="@mipmap/jigou2x"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/imageView16"
                            android:layout_alignParentTop="true"
                            android:layout_centerHorizontal="true"/>

                        <TextView
                            android:textColor="@color/black"
                            android:textSize="20dp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="机构用车"
                            android:id="@+id/textView150"
                            android:layout_below="@+id/imageView16"
                            android:layout_centerHorizontal="true"/>
                    </RelativeLayout>
                </RelativeLayout>
            </LinearLayout>

            <TextView
                android:background="@color/line_background"
                android:layout_width="match_parent"
                android:layout_height="@dimen/line_width"
                android:id="@+id/textView161"/>

            <LinearLayout
                android:layout_weight="1"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <RelativeLayout
                    android:background="@drawable/layout_selector_white_system_gray"
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_centerHorizontal="true">

                        <ImageView
                            android:src="@mipmap/main_long2x"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/imageView20"
                            android:layout_alignParentTop="true"
                            android:layout_centerHorizontal="true"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="特价优惠"
                            android:textSize="@dimen/normalTextSize"
                            android:id="@+id/textView151"
                            android:layout_below="@+id/imageView20"
                            android:layout_centerHorizontal="true"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="超值优惠低至6折"
                            android:textSize="10dp"
                            android:id="@+id/textView155"
                            android:layout_below="@+id/textView151"
                            android:layout_centerHorizontal="true"/>
                    </RelativeLayout>
                </RelativeLayout>

                <TextView
                    android:background="@color/line_background"
                    android:layout_width="@dimen/line_width"
                    android:layout_height="match_parent"
                    android:id="@+id/textView163"/>

                <RelativeLayout
                    android:background="@drawable/layout_selector_white_system_gray"
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_centerHorizontal="true">

                        <ImageView
                            android:src="@mipmap/daijia2x"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/imageView33"
                            android:layout_alignParentTop="true"
                            android:layout_centerHorizontal="true"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="代驾"
                            android:textSize="@dimen/normalTextSize"
                            android:id="@+id/textView157"
                            android:layout_below="@+id/imageView33"
                            android:layout_centerHorizontal="true"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="专业代驾，贴心安全"
                            android:textSize="10dp"
                            android:id="@+id/textView158"
                            android:layout_below="@+id/textView157"
                            android:layout_centerHorizontal="true"/>
                    </RelativeLayout>
                </RelativeLayout>

                <TextView
                    android:background="@color/line_background"
                    android:layout_width="@dimen/line_width"
                    android:layout_height="match_parent"
                    android:id="@+id/textView164"/>

                <RelativeLayout
                    android:background="@drawable/layout_selector_white_system_gray"
                    android:layout_weight="1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <RelativeLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_centerHorizontal="true">

                        <ImageView
                            android:src="@mipmap/main_store2x"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/imageView34"
                            android:layout_alignParentTop="true"
                            android:layout_centerHorizontal="true"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="门店"
                            android:textSize="@dimen/normalTextSize"
                            android:id="@+id/textView159"
                            android:layout_below="@+id/imageView34"
                            android:layout_centerHorizontal="true"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="线下实体店"
                            android:textSize="10dp"
                            android:id="@+id/textView160"
                            android:layout_below="@+id/textView159"
                            android:layout_centerHorizontal="true"/>
                    </RelativeLayout>
                </RelativeLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

</RelativeLayout>